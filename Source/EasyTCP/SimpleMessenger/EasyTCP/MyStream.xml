<EasyTCP
	Version="4.0"
	xmlns="EasyTCP"
	xsi:schemaLocation="EasyTCP ../../../EasyTCP.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<Stream ClassName="MyStream" Version="2.0">
		<Packet Code="0" Name="Info">
			<Data Name="Name" Type="string" />
		</Packet>

		<Packet Code="1" Name="Message">
			<Data Name="Message" Type="string" />
		</Packet>

		<Packet Code="2" Name="Picture">
			<Data Name="Data" Type="ByteArray" />
		</Packet>
	</Stream>
</EasyTCP>
